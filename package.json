{
  "name": "supra-autofi-agent",
  "version": "1.0.0",
  "description": "DeFi Automation AI Agent for Supra Network",
  "main": "dist/index.js",
  "bin": {
    "super-agent": "./dist/cli.js"
  },
  "scripts": {
    "start": "npm run build && node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist/",
    "prebuild": "npm run clean",
    "health": "ts-node src/health-check.ts"
  },
  "author": "Jatin Pandya, DevRel @ Supra",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^16.5.0",
    "openai": "^4.28.0",
    "readline": "^1.3.0",
    "supra-l1-sdk": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
